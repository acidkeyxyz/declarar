{
	"printWidth": 100,
  "trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": true,
  "semi": true,
	"singleQuote": true,
	"quoteProps":"as-needed",
	"bracketSpacing":false,
	"bracketSameLine":true,
	"arrowParens":"always",
	"endOfLine":"lf",
	"[html]": {
		"editor.defaultFormatter": "vscode.html-language-features"
	}
}
